{
	"name": "kysqlgen",
	"version": "0.1.2",
	"type": "module",
	"description": "Generate Kysely types from a database schema",
	"repository": {
		"type": "git",
		"url": "github.com/matt-allan/kysqlgen"
	},
	"keywords": [
		"kysely",
		"codegen",
		"mysql"
	],
	"author": "Matt Allan",
	"license": "MIT",
	"main": "./dist/index.js",
	"bin": {
		"kysqlgen": "./dist/bin.js"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./mysql": {
			"import": {
				"types": "./dist/mysql/index.d.ts",
				"default": "./dist/mysql/index.js"
			}
		},
		"./sqlite": {
			"import": {
				"types": "./dist/sqlite/index.d.ts",
				"default": "./dist/sqlite/index.js"
			}
		}
	},
	"packageManager": "npm@11.5.1",
	"engines": {
		"node": ">=v24.5.0"
	},
	"volta": {
		"node": "24.5.0",
		"npm": "11.5.1"
	},
	"scripts": {
		"clean": "tsc --clean",
		"check-types": "tsc --noEmit",
		"build": "tsc",
		"format": "biome format --fix",
		"lint": "biome lint",
		"test": "node --env-file-if-exists=.env --test",
		"fix": "biome check --write",
		"prepare": "npm run build",
		"prepublishOnly": "npm run check-types && npm run lint"
	},
	"dependencies": {
		"inflected": "^2.1.0"
	},
	"peerDependencies": {
		"kysely": ">=0.28.5 <0.29.0",
		"mysql2": ">=3.14.3 <4.0.0",
		"better-sqlite3": ">=^11.9.1 <13.0.0"
	},
	"peerDependenciesMeta": {
		"mysql2": {
			"optional": true
		},
		"better-sqlite3": {
			"optional": true
		}
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.2",
		"@types/better-sqlite3": "^7.6.13",
		"@types/inflected": "^2.1.3",
		"@types/node": "^24.3.0",
		"better-sqlite3": "^12.2.0",
		"kysely": "^0.28.5",
		"mysql2": "^3.14.3",
		"typescript": "^5.9.2"
	}
}
